# ---       DB VARS       ---
HIKARI_URL=jdbc:mysql://host.docker.internal:3306/nvip_test?useSSL=false&allowPublicKeyRetrieval=true
HIKARI_USER=root
HIKARI_PASSWORD=root

DS_CACHE_PREP_STMTS=true
DS_PREP_STMT_CACHE_SIZE=512
DS_PREP_STMT_CACHE_SQL_LIMIT=2048

# --- Log4j level. Could be INFO, WARNING or ERROR ---
