# ---       DB VARS       ---
HIKARI_URL=jdbc:mysql://localhost:3306/nvip?useSSL=false&allowPublicKeyRetrieval=true
HIKARI_USER=root
HIKARI_PASSWORD=root

DS_CACHE_PREP_STMTS=true
DS_PREP_STMT_CACHE_SIZE=512
DS_PREP_STMT_CACHE_SQL_LIMIT=2048

# --- Log4j level. Could be INFO, WARNING or ERROR ---
LOG4J_LOG_LEVEL=INFO

# ---     NVIP SUB DIRS    ---
OUTPUT_DIR=output
RESOURCE_DIR=productnameextractor/nvip_data
DATA_DIR=data
NLP_DIR=nlp

# --- Mysql or sqlite: all lowercase ---
DB_TYPE=mysql

# ---       RABBITMQ       ---
RABBIT_ENABLED=false
RABBIT_TIMEOUT=3600

# ---       PRODUCT NAME EXTRACTOR VARS     ---
CHAR_2_VEC_CONFIG=c2v_model_config_50.json
CHAR_2_VEC_WEIGHTS=c2v_model_weights_50.h5
WORD_2_VEC=w2v_model_250.bin
NER_MODEL=NERallModel.bin
NER_MODEL_NORMALIZER=NERallNorm.bin
SENTENCE_MODEL=en-sent.bin
PRODUCT_DETECTOR_MODEL=en-pos-perceptron.bin
CVE_LIMIT=1000
NUM_THREADS=12
MAX_PAGES=10
MAX_ATTEMPTS_PER_PAGE=2
PRODUCT_DICT_NAME=product_dict.json
TEST_MODE=false
PRETTY_PRINT=true