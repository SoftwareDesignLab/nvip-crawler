# ---       DB VARS       ---
DB_TYPE=mysql
HIKARI_URL=jdbc:mysql://host.docker.internal:3306/nvip?useSSL=false&allowPublicKeyRetrieval=true
HIKARI_USER=root
HIKARI_PASSWORD=root

# ---       RABBITMQ       ---
RABBIT_POLL_INTERVAL=60
RABBIT_HOST=host.docker.internal
RABBIT_PORT=5672
RABBIT_USERNAME=guest
RABBIT_PASSWORD=guest
PF_INPUT_QUEUE=PNE_OUT

# ---       PATCH FINDER VARS     ---
PF_INPUT_MODE=db
CVE_LIMIT=20
ADDRESS_BASES=https://www.github.com/,https://www.gitlab.com/
MAX_THREADS=10
CLONE_COMMIT_THRESHOLD=1000
CLONE_COMMIT_LIMIT=50000
CLONE_PATH=nvip_data/patch-repos
PATCH_SRC_URL_PATH=nvip_data/source_dict.json

# ---       FIX FINDER VARS     ---
FF_INPUT_MODE=db