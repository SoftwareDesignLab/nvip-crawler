HIKARI_URL=jdbc:mysql://host.docker.internal:3306/nvip?useSSL=false&allowPublicKeyRetrieval=true
HIKARI_USER=root
HIKARI_PASSWORD=root

LOG_LEVEL=INFO

NVIP_DATA_DIR=resources
NVIP_REFRESH_NVD_LIST=true
NVIP_NVD_URL=https://services.nvd.nist.gov/rest/json/cves/2.0?pubStartDate=<StartDate>&pubEndDate=<EndDate>

NVIP_PARALLEL_PROCESS_THREAD_LIMIT=9
NVIP_OUTPUT_DIR=output/crawlers
NVIP_SEED_URLS=resources/url-sources/nvip-seeds-test.txt
NVIP_WHITELIST_URLS=resources/url-sources/nvip-whitelist.txt
NVIP_ENABLE_GITHUB=false

NVIP_CRAWLER_POLITENESS=3000
NVIP_CRAWLER_MAX_PAGES=3000
NVIP_CRAWLER_DEPTH=1
NVIP_CRAWLER_REPORT_ENABLE=true
NVIP_NUM_OF_CRAWLER=10